{
    "version": 2,

    "routes":[
        {
            "src":"/submit",
            "dest":"/index.js",
            "methods":["POST"]
        },
        {
            "src":"/claim",
            "dest":"/index.js",
            "methods":["POST"]
        },
        {
            "src":"/(.*)",
            "dest":"/index.js"
        }
    ],

    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ]
}